# LLM Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OLLAMA_BASE_URL=http://localhost:11434

# Vector Database Configuration
CHROMA_PERSIST_DIRECTORY=./data/chroma_db
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Quantum Simulation Configuration
MAX_HILBERT_SPACE_DIM=200
DEFAULT_TRUNCATION=50
QUANTUM_BACKEND=qutip

# Agent Configuration
MAX_AGENT_ITERATIONS=10
AGENT_TIMEOUT=300
ENABLE_PARALLEL_AGENTS=true

# Optimization Configuration
GENETIC_ALGORITHM_POPULATION=50
GENETIC_ALGORITHM_GENERATIONS=100
BAYESIAN_OPTIMIZATION_ITERATIONS=50

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/agentic_quantum.log

# Performance Configuration
ENABLE_CACHING=true
CACHE_TTL=3600
MAX_CONCURRENT_SIMULATIONS=4
